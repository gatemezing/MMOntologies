    <owl:AnnotationProperty rdf:about="#mappableTo">
        <rdfs:label xml:lang="en">mappable to</rdfs:label>
    </owl:AnnotationProperty>
    <owl:AnnotationProperty rdf:about="&rdfs;label"/>
