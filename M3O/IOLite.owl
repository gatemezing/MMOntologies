    <!-- http://www.loa-cnr.it/ontologies/IOLite.owl#digitallyReproduces -->

    <owl:ObjectProperty rdf:about="#digitallyReproduces">
        <rdfs:label xml:lang="en">digitally reproduces</rdfs:label>
        <rdfs:label xml:lang="it">riproduce digitalmente</rdfs:label>
