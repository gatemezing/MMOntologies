     -->

    


    <!-- http://www.loa-cnr.it/ontologies/DUL.owl#Action -->

    <owl:Class rdf:about="&dul;Action"/>
    


    <!-- http://www.loa-cnr.it/ontologies/DUL.owl#Agent -->

    <owl:Class rdf:about="&dul;Agent">
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#isAuthorOf"/>
                <owl:allValuesFrom>
                    <owl:Class>
                        <owl:unionOf rdf:parseType="Collection">
                            <rdf:Description rdf:about="&dul;InformationObject"/>
                            <rdf:Description rdf:about="&dul;InformationRealization"/>
                        </owl:unionOf>
                    </owl:Class>
                </owl:allValuesFrom>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>
    


    <!-- http://www.loa-cnr.it/ontologies/DUL.owl#Community -->

    <owl:Class rdf:about="&dul;Community"/>
    


    <!-- http://www.loa-cnr.it/ontologies/DUL.owl#Contract -->

    <owl:Class rdf:about="&dul;Contract"/>
    


    <!-- http://www.loa-cnr.it/ontologies/DUL.owl#Description -->

    <owl:Class rdf:about="&dul;Description"/>
    


    <!-- http://www.loa-cnr.it/ontologies/DUL.owl#Entity -->

    <owl:Class rdf:about="&dul;Entity">
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#isFormallyRepresentedIn"/>
                <owl:allValuesFrom rdf:resource="#FormalExpression"/>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>
    


    <!-- http://www.loa-cnr.it/ontologies/DUL.owl#FormalEntity -->

    <owl:Class rdf:about="&dul;FormalEntity">
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#isAssignedTo"/>
                <owl:allValuesFrom rdf:resource="#FormalExpression"/>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>
    


    <!-- http://www.loa-cnr.it/ontologies/DUL.owl#InformationObject -->

    <owl:Class rdf:about="&dul;InformationObject">
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#isLexicalizedBy"/>
                <owl:allValuesFrom rdf:resource="#LinguisticObject"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#hasRepresentationLanguage"/>
                <owl:allValuesFrom rdf:resource="#Language"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#hasAuthor"/>
                <owl:allValuesFrom rdf:resource="&dul;Agent"/>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>
    


    <!-- http://www.loa-cnr.it/ontologies/DUL.owl#InformationRealization -->

    <owl:Class rdf:about="&dul;InformationRealization">
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#isCopyOf"/>
                <owl:allValuesFrom rdf:resource="&dul;InformationRealization"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#isOriginalOf"/>
                <owl:allValuesFrom rdf:resource="&dul;InformationRealization"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#isReproducedBy"/>
                <owl:allValuesFrom rdf:resource="&dul;InformationRealization"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#hasAuthor"/>
                <owl:allValuesFrom rdf:resource="&dul;Agent"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#reproduces"/>
                <owl:allValuesFrom rdf:resource="&dul;InformationRealization"/>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>
    


    <!-- http://www.loa-cnr.it/ontologies/DUL.owl#PhysicalAgent -->

    <owl:Class rdf:about="&dul;PhysicalAgent"/>
    


    <!-- http://www.loa-cnr.it/ontologies/DUL.owl#PhysicalArtifact -->

    <owl:Class rdf:about="&dul;PhysicalArtifact"/>
    


    <!-- http://www.loa-cnr.it/ontologies/DUL.owl#Plan -->

    <owl:Class rdf:about="&dul;Plan"/>
    


    <!-- http://www.loa-cnr.it/ontologies/DUL.owl#PlanExecution -->

    <owl:Class rdf:about="&dul;PlanExecution"/>
    


    <!-- http://www.loa-cnr.it/ontologies/DUL.owl#Process -->

    <owl:Class rdf:about="&dul;Process"/>
    


    <!-- http://www.loa-cnr.it/ontologies/DUL.owl#Role -->

    <owl:Class rdf:about="&dul;Role"/>
    


    <!-- http://www.loa-cnr.it/ontologies/DUL.owl#Situation -->

    <owl:Class rdf:about="&dul;Situation"/>
    


    <!-- http://www.loa-cnr.it/ontologies/DUL.owl#SocialObject -->

    <owl:Class rdf:about="&dul;SocialObject">
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#hasCulturalGrounding"/>
                <owl:allValuesFrom rdf:resource="&dul;SocialObject"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#hasSchema"/>
                <owl:allValuesFrom rdf:resource="#DataStructure"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#hasCulturalMixingWith"/>
                <owl:allValuesFrom rdf:resource="&dul;SocialObject"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#isCulturalGroundingFor"/>
                <owl:allValuesFrom rdf:resource="&dul;SocialObject"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#metaphoricallyBlendsWith"/>
                <owl:allValuesFrom rdf:resource="&dul;SocialObject"/>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>
    


    <!-- http://www.loa-cnr.it/ontologies/DUL.owl#Theory -->

    <owl:Class rdf:about="&dul;Theory"/>
    


    <!-- http://www.loa-cnr.it/ontologies/IOLite.owl#BodilyMotion -->

    <owl:Class rdf:about="#BodilyMotion">
        <rdfs:label xml:lang="en">Bodily motion</rdfs:label>
        <rdfs:label xml:lang="it">Movimento corporeo</rdfs:label>
        <rdfs:subClassOf rdf:resource="&dul;Action"/>
        <rdfs:subClassOf rdf:resource="&dul;InformationRealization"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&dul;hasParticipant"/>
                <owl:someValuesFrom rdf:resource="&dul;PhysicalAgent"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment>An information realization consisting of bodily movements.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.loa-cnr.it/ontologies/IOLite.owl#Code -->

    <owl:Class rdf:about="#Code">
        <rdfs:label xml:lang="en">Code</rdfs:label>
        <rdfs:label xml:lang="it">Codice</rdfs:label>
        <rdfs:subClassOf rdf:resource="&dul;InformationObject"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&dul;isRealizedBy"/>
                <owl:allValuesFrom rdf:resource="#DigitalResource"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment>Any piece of information expressing computational operations, objects, markup, etc.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.loa-cnr.it/ontologies/IOLite.owl#CommunicativeFunction -->

    <owl:Class rdf:about="#CommunicativeFunction">
        <rdfs:label xml:lang="en">Communicative function</rdfs:label>
        <rdfs:label xml:lang="it">Funzione comunicativa</rdfs:label>
        <rdfs:subClassOf rdf:resource="&dul;Plan"/>
        <rdfs:comment>The functions, e.g. defined by Jakobson and by Buhler, which define types of linguistic acts. Jakobson&#39;s ones include referential, conative, expressive, phatic, metalinguistic, poetic. Each function has typical roles and tasks that must be played during a linguistic act that achieves the function.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.loa-cnr.it/ontologies/IOLite.owl#ContractText -->

    <owl:Class rdf:about="#ContractText">
        <rdfs:label xml:lang="en">Contract text</rdfs:label>
        <rdfs:label xml:lang="it">Testo di un contratto</rdfs:label>
        <rdfs:subClassOf rdf:resource="#Text"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&dul;expresses"/>
                <owl:someValuesFrom rdf:resource="&dul;Contract"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment>The text of a contract</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.loa-cnr.it/ontologies/IOLite.owl#DBSchema -->

    <owl:Class rdf:about="#DBSchema">
        <rdfs:label xml:lang="en">Database schema</rdfs:label>
        <rdfs:label xml:lang="it">Schema di base di dati</rdfs:label>
        <rdfs:subClassOf rdf:resource="#DataStructure"/>
        <rdfs:comment>Any conceptual, logical or physical schema for a database.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.loa-cnr.it/ontologies/IOLite.owl#DataStructure -->

    <owl:Class rdf:about="#DataStructure">
        <rdfs:label xml:lang="en">Data structure</rdfs:label>
        <rdfs:label xml:lang="it">Struttura di dati</rdfs:label>
        <rdfs:subClassOf rdf:resource="&dul;InformationObject"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#isSchemaOf"/>
                <owl:allValuesFrom rdf:resource="&dul;SocialObject"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&dul;isRealizedBy"/>
                <owl:allValuesFrom rdf:resource="#DigitalResource"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment rdf:datatype="&xsd;string">Any data structure, including databases, schemas, lexica, knowledge organizations systems, etc.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.loa-cnr.it/ontologies/IOLite.owl#Datum -->

    <owl:Class rdf:about="#Datum">
        <rdfs:label xml:lang="en">Datum</rdfs:label>
        <rdfs:label xml:lang="it">Dato</rdfs:label>
        <rdfs:subClassOf rdf:resource="&dul;InformationObject"/>
        <rdfs:comment>From WordNet: &#39;an item of factual information derived from measurement or research&#39;</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.loa-cnr.it/ontologies/IOLite.owl#Depiction -->

    <owl:Class rdf:about="#Depiction">
        <rdfs:label xml:lang="en">Depiction</rdfs:label>
        <rdfs:label xml:lang="it">Realizzazione visiva</rdfs:label>
        <rdfs:subClassOf rdf:resource="&dul;InformationRealization"/>
        <rdfs:subClassOf rdf:resource="&dul;PhysicalArtifact"/>
        <rdfs:comment>An information realization consisting of depicted images/signs of any sort (e.g. graffiti, drawings, inscriptions, pictures, sculptures, etc.), which are inscripted on a medium that lasts longer than the depicting act.
It also includes any early form of inscripted iconic expression, which can be considered as original bodily expressions.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.loa-cnr.it/ontologies/IOLite.owl#DigitalPhoto -->

    <owl:Class rdf:about="#DigitalPhoto">
        <rdfs:label xml:lang="en">Digital photo</rdfs:label>
        <rdfs:subClassOf rdf:resource="#DigitalResource"/>
    </owl:Class>
    


    <!-- http://www.loa-cnr.it/ontologies/IOLite.owl#DigitalResource -->

    <owl:Class rdf:about="#DigitalResource">
        <rdfs:label xml:lang="en">Digital resource</rdfs:label>
        <rdfs:label xml:lang="it">Risorsa digitale</rdfs:label>
        <rdfs:subClassOf rdf:resource="&dul;InformationRealization"/>
        <rdfs:comment>Any resource that can be computed, e.g. a file, a piece of (implemented) software.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.loa-cnr.it/ontologies/IOLite.owl#Drawing -->

    <owl:Class rdf:about="#Drawing">
        <rdfs:label xml:lang="en">Drawing</rdfs:label>
        <rdfs:label xml:lang="it">Disegno</rdfs:label>
        <rdfs:subClassOf rdf:resource="#Depiction"/>
        <rdfs:comment>wn noun: A representation of forms or objects on a surface by means of lines; &quot;drawings of abstract forms&quot;; &quot;he did complicated pen-and-ink drawings like medieval miniatures&quot;</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.loa-cnr.it/ontologies/IOLite.owl#FormalExpression -->

    <owl:Class rdf:about="#FormalExpression">
        <rdfs:label xml:lang="en">Formal expression</rdfs:label>
        <rdfs:label xml:lang="it">Espressione formale</rdfs:label>
        <rdfs:subClassOf rdf:resource="&dul;InformationObject"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#isAssignmentOf"/>
                <owl:allValuesFrom rdf:resource="&dul;FormalEntity"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#hasRepresentationLanguage"/>
                <owl:someValuesFrom rdf:resource="#FormalLanguage"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#formallyRepresents"/>
                <owl:allValuesFrom rdf:resource="&dul;Entity"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#isFormalTermFor"/>
                <owl:someValuesFrom rdf:resource="&dul;InformationObject"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <owl:disjointWith rdf:resource="#KOS"/>
        <rdfs:comment>Any information object represented in a FormalLanguage, usually having a formal interpretation by a dul:FormalEntity, and used to formally represent any Entity</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.loa-cnr.it/ontologies/IOLite.owl#FormalLanguage -->

    <owl:Class rdf:about="#FormalLanguage">
        <rdfs:label xml:lang="en">Formal language</rdfs:label>
        <rdfs:label xml:lang="it">Linguaggio formale</rdfs:label>
        <rdfs:subClassOf rdf:resource="#Language"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&dul;hasPart"/>
                <owl:allValuesFrom rdf:resource="#FormalExpression"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment>A formal language, created by some human, with a fixed grammar, and usually with an explicit formal semantics (i.e. any FormalExpression that is a wff or a valid element of a FormalLanguage has an interpretation wrt to formal entities such as sets, categories, etc.).</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.loa-cnr.it/ontologies/IOLite.owl#Gesture -->

    <owl:Class rdf:about="#Gesture">
        <rdfs:label xml:lang="en">Gesture</rdfs:label>
        <rdfs:label xml:lang="it">Gesto</rdfs:label>
        <rdfs:subClassOf rdf:resource="#BodilyMotion"/>
    </owl:Class>
    


    <!-- http://www.loa-cnr.it/ontologies/IOLite.owl#Grapheme -->

    <owl:Class rdf:about="#Grapheme">
        <rdfs:label xml:lang="en">Grapheme</rdfs:label>
        <rdfs:label xml:lang="it">Grafema</rdfs:label>
        <rdfs:subClassOf rdf:resource="#LinguisticObject"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&dul;isRealizedBy"/>
                <owl:allValuesFrom rdf:resource="#Writing"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment>A part of a word as it can be realized by Writing
A spoken information realization can be &#39;about&#39; a grapheme (as in reading), but it does not &#39;realize&#39; it. Only Phoneme(s) are realized by spoken information (Voicing).
BTW, since spoken realizations are a &#39;primary&#39; code of communication, the difference between direct and indirect spoken realizations (reading) should be considered relevant.
A grapheme is not necessarily able to express a meaning (a dul:SocialObject), although it can in principle (e.g. &#39;a&#39; in English).</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.loa-cnr.it/ontologies/IOLite.owl#GraphicArt -->

    <owl:Class rdf:about="#GraphicArt">
        <rdfs:label xml:lang="en">Graphic art</rdfs:label>
        <rdfs:label xml:lang="it">Opera grafica</rdfs:label>
        <rdfs:subClassOf rdf:resource="#Depiction"/>
        <rdfs:comment>wn noun: The arts of drawing or painting or printmaking</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.loa-cnr.it/ontologies/IOLite.owl#IconicLanguage -->

    <owl:Class rdf:about="#IconicLanguage">
        <rdfs:label xml:lang="en">Iconic language</rdfs:label>
        <rdfs:label xml:lang="it">Linguaggio iconico</rdfs:label>
        <rdfs:subClassOf rdf:resource="#Language"/>
        <rdfs:comment>A language made up of graphical elements. It can be natural, artificial, and even formal.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.loa-cnr.it/ontologies/IOLite.owl#IconicObject -->

    <owl:Class rdf:about="#IconicObject">
        <rdfs:label xml:lang="en">Iconic object</rdfs:label>
        <rdfs:label xml:lang="it">Oggetto iconico</rdfs:label>
        <rdfs:subClassOf rdf:resource="&dul;InformationObject"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#hasRepresentationLanguage"/>
                <owl:someValuesFrom rdf:resource="#IconicLanguage"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment>An information object represented in an IconicLanguage</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.loa-cnr.it/ontologies/IOLite.owl#KOS -->

    <owl:Class rdf:about="#KOS">
        <rdfs:label xml:lang="en">Knowledge organization system</rdfs:label>
        <rdfs:label xml:lang="it">Sistema di organizzazione della conoscenza</rdfs:label>
        <rdfs:subClassOf rdf:resource="#DataStructure"/>
        <rdfs:comment>Knowledge Organization Systems: thesauri, terminologies, classification schemes, subject hierarchies, etc.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.loa-cnr.it/ontologies/IOLite.owl#Language -->

    <owl:Class rdf:about="#Language">
        <rdfs:label xml:lang="en">Language</rdfs:label>
        <rdfs:label xml:lang="it">Linguaggio</rdfs:label>
        <rdfs:subClassOf rdf:resource="&dul;Description"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#isRepresentationLanguageOf"/>
                <owl:allValuesFrom rdf:resource="&dul;InformationObject"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment>A natural or artificial language, provided with an alphabet (or vocabulary) and combinatorial rules. In the case of natural languages, their components are &#39;temporary&#39; and &#39;reconstructed&#39; out of actual usage. For example, a grammar for a natural language has the status of a theory for that language, and alternative ones can exist (e.g. generative vs. construction grammars).
Another distinction, between the general (systemic) rules for a language, and the local (contextual) rules for e.g. a certain context, speaker, place, etc., can be made separately.
The most comprehensive classification of languages ha probably been made by Umberto Eco, based on the production modes of the &#39;signs&#39; that are represented in a certain language. It uses several semiotic dimensions, and will be modeled in a forthcoming ontology.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.loa-cnr.it/ontologies/IOLite.owl#Lexeme -->

    <owl:Class rdf:about="#Lexeme">
        <rdfs:label xml:lang="en">Lexeme</rdfs:label>
        <rdfs:label xml:lang="it">Lessema</rdfs:label>
        <rdfs:subClassOf rdf:resource="#Term"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#hasSchema"/>
                <owl:someValuesFrom rdf:resource="#Lexicon"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="#Multiword"/>
                    <rdf:Description rdf:about="#Word"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:subClassOf>
        <rdfs:comment>Lexical entries for dictionaries, lexica, etc. They are used to create reference forms of words.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.loa-cnr.it/ontologies/IOLite.owl#Lexicon -->

    <owl:Class rdf:about="#Lexicon">
        <rdfs:label xml:lang="en">Lexicon</rdfs:label>
        <rdfs:label xml:lang="it">Lessico</rdfs:label>
        <rdfs:subClassOf rdf:resource="#KOS"/>
        <rdfs:comment rdf:datatype="&xsd;string">A collection of lexical items (terms, entries, ...) that are witnessed to have a linguistic relevance.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.loa-cnr.it/ontologies/IOLite.owl#LinguisticAct -->

    <owl:Class rdf:about="#LinguisticAct">
        <rdfs:label xml:lang="en">Linguistic act</rdfs:label>
        <rdfs:label xml:lang="it">Atto linguistico</rdfs:label>
        <rdfs:subClassOf rdf:resource="&dul;PlanExecution"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&dul;includesObject"/>
                <owl:someValuesFrom rdf:resource="&dul;Agent"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&dul;satisfies"/>
                <owl:allValuesFrom rdf:resource="#CommunicativeFunction"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&dul;isSettingFor"/>
                <owl:someValuesFrom rdf:resource="&dul;InformationRealization"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&dul;isSettingFor"/>
                <owl:someValuesFrom>
                    <owl:Class>
                        <owl:intersectionOf rdf:parseType="Collection">
                            <rdf:Description rdf:about="&dul;Entity"/>
                            <owl:Restriction>
                                <owl:onProperty rdf:resource="&dul;isReferenceOf"/>
                                <owl:minCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:minCardinality>
                            </owl:Restriction>
                        </owl:intersectionOf>
                    </owl:Class>
                </owl:someValuesFrom>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&dul;includesObject"/>
                <owl:someValuesFrom rdf:resource="#LinguisticObject"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&dul;includesObject"/>
                <owl:someValuesFrom rdf:resource="&dul;SocialObject"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment>A communicative situation including linguistic objects, agents, and a set of contexts: physical (informational realizations), conceptual (social objects), and referential (entities).
A linguistic act has an associated CommunicativeFunction that it satisfies.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.loa-cnr.it/ontologies/IOLite.owl#LinguisticFunction -->

    <owl:Class rdf:about="#LinguisticFunction">
        <rdfs:label xml:lang="en">Linguistic function</rdfs:label>
        <rdfs:label xml:lang="it">Funzione linguistica</rdfs:label>
        <rdfs:subClassOf rdf:resource="&dul;Role"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&dul;isDefinedIn"/>
                <owl:someValuesFrom rdf:resource="#LinguisticTheory"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment>Any linguistic function that classifies words according to a LinguisticTheory. 
This class includes parts of speech, thematic roles, phrase structure components, verbal aspects, etc. e.g. Subject, Object, Instrument, Stative, etc.
Each linguistic function must be defined in a LinguisticTheory; e.g. a thematic role can be defined either in a generative grammar, or in construction-based theory.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.loa-cnr.it/ontologies/IOLite.owl#LinguisticObject -->

    <owl:Class rdf:about="#LinguisticObject">
        <rdfs:label xml:lang="en">Linguistic object</rdfs:label>
        <rdfs:label xml:lang="it">Oggetto linguistico</rdfs:label>
        <rdfs:subClassOf rdf:resource="&dul;InformationObject"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#lexicalizes"/>
                <owl:allValuesFrom rdf:resource="&dul;InformationObject"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#hasRepresentationLanguage"/>
                <owl:someValuesFrom rdf:resource="#NaturalLanguage"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment>An information object represented in a NaturalLanguage</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.loa-cnr.it/ontologies/IOLite.owl#LinguisticTheory -->

    <owl:Class rdf:about="#LinguisticTheory">
        <rdfs:label xml:lang="en">Linguistic theory</rdfs:label>
        <rdfs:label xml:lang="it">Teoria linguistica</rdfs:label>
        <rdfs:subClassOf rdf:resource="&dul;Theory"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&dul;describes"/>
                <owl:someValuesFrom>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="&dul;isComponentOf"/>
                        <owl:someValuesFrom rdf:resource="#NaturalLanguage"/>
                    </owl:Restriction>
                </owl:someValuesFrom>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment>Any theory describing the structure and/or production and understanding of a natural language or a set of natural languages, or a component of one or more natural languages.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.loa-cnr.it/ontologies/IOLite.owl#Morpheme -->

    <owl:Class rdf:about="#Morpheme">
