<rdf:RDF xmlns="http://www.loa-cnr.it/ontologies/IOLite.owl#"
     xml:base="http://www.loa-cnr.it/ontologies/IOLite.owl"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:dul="http://www.loa-cnr.it/ontologies/DUL.owl#"
