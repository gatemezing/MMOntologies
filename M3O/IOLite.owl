        <rdfs:label xml:lang="it">fusoMetaforicamenteCon</rdfs:label>
        <rdfs:domain rdf:resource="&dul;SocialObject"/>
        <rdfs:range rdf:resource="&dul;SocialObject"/>
        <owl:inverseOf rdf:resource="#metaphoricallyBlendsWith"/>
    </owl:SymmetricProperty>
    


