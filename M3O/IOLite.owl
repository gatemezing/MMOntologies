        <rdfs:label xml:lang="it">&#232; l&#39;originale di</rdfs:label>
        <rdfs:range rdf:resource="&dul;InformationRealization"/>
        <rdfs:domain rdf:resource="&dul;InformationRealization"/>
    </owl:ObjectProperty>
    


    <!-- http://www.loa-cnr.it/ontologies/IOLite.owl#isRepresentationLanguageOf -->

    <owl:ObjectProperty rdf:about="#isRepresentationLanguageOf">
