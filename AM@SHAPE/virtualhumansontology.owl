<?xml version="1.0"?>


<!DOCTYPE rdf:RDF [
    <!ENTITY owl "http://www.w3.org/2002/07/owl#" >
    <!ENTITY dc "http://purl.org/dc/elements/1.1/" >
    <!ENTITY xsd "http://www.w3.org/2001/XMLSchema#" >
    <!ENTITY daml "http://www.daml.org/2001/03/daml+oil#" >
    <!ENTITY rdfs "http://www.w3.org/2000/01/rdf-schema#" >
    <!ENTITY rdf "http://www.w3.org/1999/02/22-rdf-syntax-ns#" >
    <!ENTITY ico "http://www.aimatshape.net/ontologies/infoCommonOntology.owl#" >
    <!ENTITY cto "http://www.aimatshape.net/ontologies/toolCommonOntology.owl#" >
    <!ENTITY cso "http://www.aimatshape.net/ontologies/shapeCommonOntology.owl#" >
]>


<rdf:RDF xmlns="http://www.aimatshape.net/ontologies/virtualHumansOntology.owl#"
     xml:base="http://www.aimatshape.net/ontologies/virtualHumansOntology.owl"
     xmlns:dc="http://purl.org/dc/elements/1.1/"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:cto="http://www.aimatshape.net/ontologies/toolCommonOntology.owl#"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:ico="http://www.aimatshape.net/ontologies/infoCommonOntology.owl#"
     xmlns:cso="http://www.aimatshape.net/ontologies/shapeCommonOntology.owl#"
     xmlns:daml="http://www.daml.org/2001/03/daml+oil#">
    <owl:Ontology rdf:about="">
        <owl:imports rdf:resource="file:/C:/infoOntology_v1.0.owl"/>
        <owl:imports rdf:resource="file:/C:/shapeOntology_v2.3.1.owl"/>
        <owl:imports rdf:resource="file:/C:/toolOntology_v1.12.1.owl"/>
    </owl:Ontology>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Annotation properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    <owl:AnnotationProperty rdf:about="&rdfs;comment"/>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.aimatshape.net/ontologies/virtualHumansOntology.owl#IsPartOf -->

    <owl:ObjectProperty rdf:about="#IsPartOf">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:domain rdf:resource="#ClothPattern"/>
        <rdfs:range rdf:resource="#Garment"/>
        <owl:inverseOf rdf:resource="#isComprisedOf"/>
    </owl:ObjectProperty>
    


    <!-- http://www.aimatshape.net/ontologies/virtualHumansOntology.owl#beolongsToModelPart -->

    <owl:TransitiveProperty rdf:about="#beolongsToModelPart">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdf:type rdf:resource="&owl;ObjectProperty"/>
        <rdfs:comment rdf:datatype="&xsd;string">Describes to which model part the joint belongs to. e.g. the joint r_hip belongs to the Right Leg part.</rdfs:comment>
        <rdfs:range rdf:resource="#ModelPart"/>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="#BodyPartSize"/>
                    <rdf:Description rdf:about="#Node"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
    </owl:TransitiveProperty>
    


    <!-- http://www.aimatshape.net/ontologies/virtualHumansOntology.owl#canPerfromAnimation -->

    <owl:ObjectProperty rdf:about="#canPerfromAnimation">
        <rdfs:comment rdf:datatype="&xsd;string"></rdfs:comment>
        <rdfs:range rdf:resource="#AnimationSequence"/>
        <rdfs:domain rdf:resource="#VirtualHuman"/>
    </owl:ObjectProperty>
    


    <!-- http://www.aimatshape.net/ontologies/virtualHumansOntology.owl#canUseObject -->

    <owl:ObjectProperty rdf:about="#canUseObject">
        <rdfs:range rdf:resource="#SmartObject"/>
        <rdfs:domain rdf:resource="#VirtualHuman"/>
    </owl:ObjectProperty>
    


    <!-- http://www.aimatshape.net/ontologies/virtualHumansOntology.owl#controls -->

    <owl:ObjectProperty rdf:about="#controls">
        <rdfs:comment rdf:datatype="&xsd;string">This is to describe the characteristics of the kind of Virtual Human  the controller is designed for.</rdfs:comment>
        <rdfs:range rdf:resource="#VirtualHuman"/>
        <rdfs:domain rdf:resource="#VirtualHumanController"/>
        <owl:inverseOf rdf:resource="#isControlledBy"/>
    </owl:ObjectProperty>
    


    <!-- http://www.aimatshape.net/ontologies/virtualHumansOntology.owl#has -->

    <owl:TransitiveProperty rdf:about="#has">
        <rdf:type rdf:resource="&owl;ObjectProperty"/>
        <rdfs:comment rdf:datatype="&xsd;string">This propoerty groups all VH properties</rdfs:comment>
        <rdfs:domain rdf:resource="#VirtualHuman"/>
    </owl:TransitiveProperty>
    


    <!-- http://www.aimatshape.net/ontologies/virtualHumansOntology.owl#hasActorMorphology -->

    <owl:ObjectProperty rdf:about="#hasActorMorphology">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:comment rdf:datatype="&xsd;string">Describes the morphology of the person that performed the mocap sequence.</rdfs:comment>
        <rdfs:range rdf:resource="#MorphologyDescription"/>
        <rdfs:domain rdf:resource="#MotionCaptureSequence"/>
    </owl:ObjectProperty>
    


    <!-- http://www.aimatshape.net/ontologies/virtualHumansOntology.owl#hasAlgorthm -->

    <owl:ObjectProperty rdf:about="#hasAlgorthm">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:range rdf:resource="&cto;Algorithm"/>
        <rdfs:domain rdf:resource="#VirtualHumanController"/>
    </owl:ObjectProperty>
    


    <!-- http://www.aimatshape.net/ontologies/virtualHumansOntology.owl#hasAnimationParameter -->

    <owl:ObjectProperty rdf:about="#hasAnimationParameter">
        <rdfs:comment rdf:datatype="&xsd;string">For MPEG-4 only. This is to define each transformation parameter of the animation, for the body or the face. This is useful in the case the users want to perform an exahustive analysis of the data.</rdfs:comment>
        <rdfs:domain rdf:resource="#KeyFrameSequence"/>
        <rdfs:range>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="#BodyAnimationParameter"/>
                    <rdf:Description rdf:about="#FacialAnimationParameter"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:range>
    </owl:ObjectProperty>
    


    <!-- http://www.aimatshape.net/ontologies/virtualHumansOntology.owl#hasAttribute -->

    <owl:ObjectProperty rdf:about="#hasAttribute">
        <rdfs:range rdf:resource="#ObjectAttribute"/>
        <rdfs:domain rdf:resource="#SmartObject"/>
    </owl:ObjectProperty>
    


    <!-- http://www.aimatshape.net/ontologies/virtualHumansOntology.owl#hasAuthor -->

    <owl:ObjectProperty rdf:about="#hasAuthor">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:domain rdf:resource="#Resource"/>
        <rdfs:range>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="&ico;InstitutionInfo"/>
                    <rdf:Description rdf:about="&ico;PersonInfo"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:range>
    </owl:ObjectProperty>
    


    <!-- http://www.aimatshape.net/ontologies/virtualHumansOntology.owl#hasBAPDefinition -->

    <owl:ObjectProperty rdf:about="#hasBAPDefinition">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:comment rdf:datatype="&xsd;string">The corresponding BAP concept of the given BAP value</rdfs:comment>
        <rdfs:domain rdf:resource="#BodyAnimationParameter"/>
        <rdfs:range rdf:resource="#BodyAnimationParameterDefinition"/>
    </owl:ObjectProperty>
    


    <!-- http://www.aimatshape.net/ontologies/virtualHumansOntology.owl#hasBodyPartSize -->

    <owl:ObjectProperty rdf:about="#hasBodyPartSize">
        <rdfs:comment rdf:datatype="&xsd;string"></rdfs:comment>
        <rdfs:range rdf:resource="#BodyPartSize"/>
        <rdfs:domain rdf:resource="#MorphologyDescription"/>
    </owl:ObjectProperty>
    


    <!-- http://www.aimatshape.net/ontologies/virtualHumansOntology.owl#hasCenter -->

    <owl:ObjectProperty rdf:about="#hasCenter">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:comment rdf:datatype="&xsd;string">3D vector of the center</rdfs:comment>
        <rdfs:range rdf:resource="#_3DVector"/>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="#HandPosture"/>
                    <rdf:Description rdf:about="#Location"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
    </owl:ObjectProperty>
    


    <!-- http://www.aimatshape.net/ontologies/virtualHumansOntology.owl#hasChild -->

    <owl:TransitiveProperty rdf:about="#hasChild">
        <rdf:type rdf:resource="&owl;ObjectProperty"/>
        <rdfs:domain rdf:resource="#Node"/>
        <rdfs:range rdf:resource="#Node"/>
    </owl:TransitiveProperty>
    


    <!-- http://www.aimatshape.net/ontologies/virtualHumansOntology.owl#hasDimension -->

    <owl:ObjectProperty rdf:about="#hasDimension">
        <rdfs:domain rdf:resource="#Personality"/>
        <rdfs:range rdf:resource="#PersonalityDimension"/>
    </owl:ObjectProperty>
    


    <!-- http://www.aimatshape.net/ontologies/virtualHumansOntology.owl#hasEmotionDescription -->

    <owl:ObjectProperty rdf:about="#hasEmotionDescription">
        <rdfs:domain rdf:resource="#BodyPartMovement"/>
        <rdfs:range rdf:resource="#EmotionalState"/>
    </owl:ObjectProperty>
    


    <!-- http://www.aimatshape.net/ontologies/virtualHumansOntology.owl#hasExpression -->

    <owl:ObjectProperty rdf:about="#hasExpression">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:comment rdf:datatype="&xsd;string">Emotional expression that could describe the movement</rdfs:comment>
        <rdfs:domain rdf:resource="#BodyPartMovement"/>
        <rdfs:range rdf:resource="#Expression"/>
    </owl:ObjectProperty>
    


    <!-- http://www.aimatshape.net/ontologies/virtualHumansOntology.owl#hasFAPDefinition -->

    <owl:ObjectProperty rdf:about="#hasFAPDefinition">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:comment rdf:datatype="&xsd;string">The corresponding FAP concept of the given FAP value or range of values</rdfs:comment>
        <rdfs:range rdf:resource="#FacialAnimationParameterDefinition"/>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="#FacialAnimationParameter"/>
                    <rdf:Description rdf:about="#FacialAnimationParameterRange"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
    </owl:ObjectProperty>
    


    <!-- http://www.aimatshape.net/ontologies/virtualHumansOntology.owl#hasFAPRange -->

    <owl:ObjectProperty rdf:about="#hasFAPRange">
        <rdfs:comment rdf:datatype="&xsd;string">A set of FAPs range values that are identified as a Facial Expression Profile which shows an emotion.</rdfs:comment>
        <rdfs:domain rdf:resource="#ArchetypalExpressionProfile"/>
        <rdfs:range rdf:resource="#FacialAnimationParameterRange"/>
    </owl:ObjectProperty>
    


    <!-- http://www.aimatshape.net/ontologies/virtualHumansOntology.owl#hasGeometry -->

    <owl:ObjectProperty rdf:about="#hasGeometry">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:range rdf:resource="#Geometry"/>
        <rdfs:subPropertyOf rdf:resource="#has"/>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="#Segment"/>
                    <rdf:Description rdf:about="#SmartObject"/>
                    <rdf:Description rdf:about="#VirtualHuman"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
    </owl:ObjectProperty>
    


    <!-- http://www.aimatshape.net/ontologies/virtualHumansOntology.owl#hasHeight -->

    <owl:ObjectProperty rdf:about="#hasHeight">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:comment rdf:datatype="&xsd;string">centimeters</rdfs:comment>
        <rdfs:range rdf:resource="#Measurement"/>
        <rdfs:domain rdf:resource="#MorphologyDescription"/>
    </owl:ObjectProperty>
    


    <!-- http://www.aimatshape.net/ontologies/virtualHumansOntology.owl#hasImplementationTool -->

    <owl:ObjectProperty rdf:about="#hasImplementationTool">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:comment rdf:datatype="&xsd;string">Link with upper ontology, the Comon Tools Ontology</rdfs:comment>
        <rdfs:range rdf:resource="&cto;SoftwareTool"/>
        <rdfs:domain rdf:resource="#VirtualHumanController"/>
    </owl:ObjectProperty>
    


    <!-- http://www.aimatshape.net/ontologies/virtualHumansOntology.owl#hasIndividualityDescription -->

    <owl:ObjectProperty rdf:about="#hasIndividualityDescription">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:range rdf:resource="#IndividualDescription"/>
        <rdfs:domain rdf:resource="#VirtualHuman"/>
        <rdfs:subPropertyOf rdf:resource="#has"/>
    </owl:ObjectProperty>
    


    <!-- http://www.aimatshape.net/ontologies/virtualHumansOntology.owl#hasLandmark -->

    <owl:ObjectProperty rdf:about="#hasLandmark">
        <rdfs:domain rdf:resource="#ClothPattern"/>
        <rdfs:range rdf:resource="#Landmark"/>
    </owl:ObjectProperty>
    


    <!-- http://www.aimatshape.net/ontologies/virtualHumansOntology.owl#hasLocation -->

    <owl:ObjectProperty rdf:about="#hasLocation">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:comment rdf:datatype="&xsd;string">3D vector location of the node</rdfs:comment>
        <rdfs:domain rdf:resource="#Node"/>
        <rdfs:range rdf:resource="#_3DVector"/>
    </owl:ObjectProperty>
    


    <!-- http://www.aimatshape.net/ontologies/virtualHumansOntology.owl#hasMorphologyDescription -->

    <owl:ObjectProperty rdf:about="#hasMorphologyDescription">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:range rdf:resource="#MorphologyDescription"/>
        <rdfs:domain rdf:resource="#VirtualHuman"/>
        <rdfs:subPropertyOf rdf:resource="#has"/>
    </owl:ObjectProperty>
    


    <!-- http://www.aimatshape.net/ontologies/virtualHumansOntology.owl#hasMotionCategory -->

    <owl:ObjectProperty rdf:about="#hasMotionCategory">
        <rdfs:domain rdf:resource="#AnimationSequence"/>
        <rdfs:range rdf:resource="#MotionCategory"/>
    </owl:ObjectProperty>
    


    <!-- http://www.aimatshape.net/ontologies/virtualHumansOntology.owl#hasPatternGeometry -->

    <owl:ObjectProperty rdf:about="#hasPatternGeometry">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:domain rdf:resource="#ClothPattern"/>
        <rdfs:range rdf:resource="#Geometry"/>
    </owl:ObjectProperty>
    


    <!-- http://www.aimatshape.net/ontologies/virtualHumansOntology.owl#hasProfile -->

    <owl:ObjectProperty rdf:about="#hasProfile">
        <rdfs:comment rdf:datatype="&xsd;string">A specific facial expression that can be defined inside a Profile.</rdfs:comment>
        <rdfs:range rdf:resource="#ArchetypalExpressionProfile"/>
        <rdfs:domain rdf:resource="#FacialExpression"/>
    </owl:ObjectProperty>
    


    <!-- http://www.aimatshape.net/ontologies/virtualHumansOntology.owl#hasRootNode -->

    <owl:TransitiveProperty rdf:about="#hasRootNode">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdf:type rdf:resource="&owl;ObjectProperty"/>
        <rdfs:comment rdf:datatype="&xsd;string">The starting node of the skeletal structure</rdfs:comment>
        <rdfs:range rdf:resource="#Node"/>
        <rdfs:domain rdf:resource="#StructureDescription"/>
    </owl:TransitiveProperty>
    


    <!-- http://www.aimatshape.net/ontologies/virtualHumansOntology.owl#hasShapeRepresentation -->

    <owl:TransitiveProperty rdf:about="#hasShapeRepresentation">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdf:type rdf:resource="&owl;ObjectProperty"/>
        <rdfs:comment rdf:datatype="&xsd;string">Link with upper ontology, the Comon Shape Ontology.</rdfs:comment>
        <rdfs:range rdf:resource="&cso;ShapeRepresentationAndDescription"/>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="#AnimationSequence"/>
                    <rdf:Description rdf:about="#Geometry"/>
                    <rdf:Description rdf:about="#StructureDescription"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
    </owl:TransitiveProperty>
    


    <!-- http://www.aimatshape.net/ontologies/virtualHumansOntology.owl#hasSize -->

    <owl:ObjectProperty rdf:about="#hasSize">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:range rdf:resource="#Measurement"/>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="#BodyPartSize"/>
                    <rdf:Description rdf:about="#Segment"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
    </owl:ObjectProperty>
    


    <!-- http://www.aimatshape.net/ontologies/virtualHumansOntology.owl#hasSkinning -->

    <owl:ObjectProperty rdf:about="#hasSkinning">
        <rdfs:range rdf:resource="#Skinning"/>
        <rdfs:domain rdf:resource="#VirtualHuman"/>
        <rdfs:subPropertyOf rdf:resource="#has"/>
    </owl:ObjectProperty>
    


    <!-- http://www.aimatshape.net/ontologies/virtualHumansOntology.owl#hasSmartObjectStructure -->

    <owl:ObjectProperty rdf:about="#hasSmartObjectStructure">
        <rdfs:comment rdf:datatype="&xsd;string">If the object has an internal structure such as a skeleton.</rdfs:comment>
        <rdfs:domain rdf:resource="#SmartObject"/>
        <rdfs:range rdf:resource="#SmartObjectStructure"/>
    </owl:ObjectProperty>
    


    <!-- http://www.aimatshape.net/ontologies/virtualHumansOntology.owl#hasVirtualHumanStructure -->

    <owl:ObjectProperty rdf:about="#hasVirtualHumanStructure">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:comment rdf:datatype="&xsd;string">Describe the skeletal structure of the virtula human if it does have one.</rdfs:comment>
        <rdfs:domain rdf:resource="#VirtualHuman"/>
        <rdfs:range rdf:resource="#VirtualHumanStructure"/>
        <rdfs:subPropertyOf rdf:resource="#has"/>
    </owl:ObjectProperty>
    


    <!-- http://www.aimatshape.net/ontologies/virtualHumansOntology.owl#hasWeight -->

    <owl:ObjectProperty rdf:about="#hasWeight">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:comment rdf:datatype="&xsd;string">Kilograms</rdfs:comment>
        <rdfs:range rdf:resource="#Measurement"/>
        <rdfs:domain rdf:resource="#MorphologyDescription"/>
    </owl:ObjectProperty>
    


    <!-- http://www.aimatshape.net/ontologies/virtualHumansOntology.owl#isBasedOn -->

    <owl:ObjectProperty rdf:about="#isBasedOn">
        <rdfs:comment rdf:datatype="&xsd;string">A Virtual human that was created using others Virtual Humans.</rdfs:comment>
        <rdfs:domain rdf:resource="#VirtualHuman"/>
        <rdfs:range rdf:resource="#VirtualHuman"/>
    </owl:ObjectProperty>
    


    <!-- http://www.aimatshape.net/ontologies/virtualHumansOntology.owl#isComposedOf -->

    <owl:ObjectProperty rdf:about="#isComposedOf">
        <rdfs:domain rdf:resource="#AnimationSequence"/>
        <rdfs:range rdf:resource="#BodyPartMovement"/>
    </owl:ObjectProperty>
    


    <!-- http://www.aimatshape.net/ontologies/virtualHumansOntology.owl#isComprisedOf -->

    <owl:ObjectProperty rdf:about="#isComprisedOf">
        <rdf:type rdf:resource="&owl;InverseFunctionalProperty"/>
        <rdfs:range rdf:resource="#ClothPattern"/>
        <rdfs:domain rdf:resource="#Garment"/>
    </owl:ObjectProperty>
    


    <!-- http://www.aimatshape.net/ontologies/virtualHumansOntology.owl#isControlledBy -->

    <owl:ObjectProperty rdf:about="#isControlledBy">
        <rdfs:comment rdf:datatype="&xsd;string">This is to descrine the characteristics  of Virtual Human that a controller is designed for.</rdfs:comment>
        <rdfs:domain rdf:resource="#VirtualHuman"/>
        <rdfs:range rdf:resource="#VirtualHumanController"/>
    </owl:ObjectProperty>
    


    <!-- http://www.aimatshape.net/ontologies/virtualHumansOntology.owl#isLyingOn -->

    <owl:ObjectProperty rdf:about="#isLyingOn">
        <rdfs:domain rdf:resource="#ClothPattern"/>
        <rdfs:range rdf:resource="#HumanPart"/>
    </owl:ObjectProperty>
    


    <!-- http://www.aimatshape.net/ontologies/virtualHumansOntology.owl#isSewedWith -->

    <owl:ObjectProperty rdf:about="#isSewedWith">
        <rdfs:comment rdf:datatype="&xsd;string"></rdfs:comment>
        <rdfs:range rdf:resource="#ClothPattern"/>
        <rdfs:domain rdf:resource="#ClothPattern"/>
    </owl:ObjectProperty>
    


    <!-- http://www.aimatshape.net/ontologies/virtualHumansOntology.owl#performInBodyPart -->

    <owl:ObjectProperty rdf:about="#performInBodyPart">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:domain rdf:resource="#BodyPartMovement"/>
        <rdfs:range rdf:resource="#HumanPart"/>
    </owl:ObjectProperty>
    


    <!-- http://www.aimatshape.net/ontologies/virtualHumansOntology.owl#requires -->

    <owl:ObjectProperty rdf:about="#requires">
        <rdfs:domain rdf:resource="#VirtualHumanController"/>
        <rdfs:range>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="#AnimationSequence"/>
                    <rdf:Description rdf:about="#IndividualDescription"/>
                    <rdf:Description rdf:about="#Resource"/>
                    <rdf:Description rdf:about="#VirtualHumanController"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:range>
    </owl:ObjectProperty>
    


    <!-- http://www.aimatshape.net/ontologies/virtualHumansOntology.owl#usesBDP -->

    <owl:ObjectProperty rdf:about="#usesBDP">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:domain rdf:resource="#BodyAnimationParameterDefinition"/>
        <rdfs:range rdf:resource="#BodyDefinitionParameter"/>
    </owl:ObjectProperty>
    


    <!-- http://www.aimatshape.net/ontologies/virtualHumansOntology.owl#usesFDP -->

    <owl:ObjectProperty rdf:about="#usesFDP">
        <rdfs:domain rdf:resource="#FacialAnimationParameterDefinition"/>
        <rdfs:range rdf:resource="#FacialDefinitionParameter"/>
    </owl:ObjectProperty>
    


    <!-- http://www.aimatshape.net/ontologies/virtualHumansOntology.owl#wearsGarment -->

    <owl:ObjectProperty rdf:about="#wearsGarment">
        <rdfs:comment rdf:datatype="&xsd;string"></rdfs:comment>
        <rdfs:range rdf:resource="#Garment"/>
        <rdfs:domain rdf:resource="#VirtualHuman"/>
        <rdfs:subPropertyOf rdf:resource="#has"/>
    </owl:ObjectProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Data properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.aimatshape.net/ontologies/virtualHumansOntology.owl#X -->

    <owl:FunctionalProperty rdf:about="#X">
        <rdf:type rdf:resource="&owl;DatatypeProperty"/>
        <rdfs:domain rdf:resource="#_3DVector"/>
        <rdfs:range rdf:resource="&xsd;float"/>
    </owl:FunctionalProperty>
    


    <!-- http://www.aimatshape.net/ontologies/virtualHumansOntology.owl#Y -->

    <owl:FunctionalProperty rdf:about="#Y">
        <rdf:type rdf:resource="&owl;DatatypeProperty"/>
        <rdfs:domain rdf:resource="#_3DVector"/>
        <rdfs:range rdf:resource="&xsd;float"/>
    </owl:FunctionalProperty>
    


    <!-- http://www.aimatshape.net/ontologies/virtualHumansOntology.owl#Z -->

    <owl:FunctionalProperty rdf:about="#Z">
        <rdf:type rdf:resource="&owl;DatatypeProperty"/>
        <rdfs:domain rdf:resource="#_3DVector"/>
        <rdfs:range rdf:resource="&xsd;float"/>
    </owl:FunctionalProperty>
    


    <!-- http://www.aimatshape.net/ontologies/virtualHumansOntology.owl#describesEmotion -->

    <owl:FunctionalProperty rdf:about="#describesEmotion">
        <rdf:type rdf:resource="&owl;DatatypeProperty"/>
        <rdfs:comment rdf:datatype="&xsd;string">Describes a list of human emotion that defines an emotional state. Different theorist have proposed a list of basic emotions. In this list we are describing Plutchik theory.

anger | fear | sadness | joy | disgust | surprise | curiosity | acceptance</rdfs:comment>
        <rdfs:domain rdf:resource="#EmotionalState"/>
