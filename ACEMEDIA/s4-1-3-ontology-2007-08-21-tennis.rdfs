  <rdfs:Class rdf:about="http://www.acemedia.org/ontologies/TENNIS#SlowCourt">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Courts that grab the ball on impact, slowing the bounce</rdfs:comment>
    <rdfs:subClassOf>
      <rdfs:Class rdf:about="http://www.acemedia.org/ontologies/TENNIS#Court"/>
    </rdfs:subClassOf>
  </rdfs:Class>
  <rdfs:Class rdf:about="http://www.acemedia.org/ontologies/TENNIS#Court">
