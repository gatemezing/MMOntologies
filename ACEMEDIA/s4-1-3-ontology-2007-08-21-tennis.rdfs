      <rdfs:Class rdf:about="http://www.acemedia.org/ontologies/TENNIS#Stroke"/>
    </rdfs:subClassOf>
  </rdfs:Class>
  <rdfs:Class rdf:about="http://www.acemedia.org/ontologies/TENNIS#Match">
