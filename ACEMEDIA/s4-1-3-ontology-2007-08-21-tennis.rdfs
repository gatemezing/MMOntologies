  <rdfs:Class rdf:about="http://www.acemedia.org/ontologies/TENNIS#Point">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Each point is started by a player who initiates play by "serving" the ball into a designated area on the other side of the court</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://www.acemedia.org/ontologies/MIDLEVEL-SPORT#SportGame"/>
  </rdfs:Class>
