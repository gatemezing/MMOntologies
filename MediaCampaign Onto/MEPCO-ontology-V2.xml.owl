        <rdfs:subClassOf rdf:resource="&mepco;WebSite"/>
        <rdfs:comment>The Internet version/edition of a Media Instance, i.e. nbc.com as a version of nbc channel.</rdfs:comment>
    </owl:Class>
    


    <!-- http://proton.semanticweb.org/2006/05/mepco#WebSite -->

    <owl:Class rdf:about="&mepco;WebSite">
        <rdfs:label>Web site</rdfs:label>
        <rdfs:subClassOf rdf:resource="&mepco;Internet"/>
