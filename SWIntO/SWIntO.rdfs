	 rdfs:label="smartdns:ABOUTNESS-OF">
	<rdfs:comment>Inverse of ABOUT
</rdfs:comment>
	<rdfs:subPropertyOf rdf:resource="&smartdns;REFERENCED-BY"/>
	<rdfs:range rdf:resource="&smartdns;information-object"/>
	<rdfs:domain rdf:resource="&smartdolce;entity"/>
</rdf:Property>
