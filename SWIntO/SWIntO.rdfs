	<rdfs:domain rdf:resource="&smartsumo;Movie"/>
	<rdfs:range rdf:resource="&rdfs;Literal"/>
</rdf:Property>
<rdf:Property rdf:about="&smartsumo;part"
	 rdfs:label="smartsumo:part"/>
<rdf:Property rdf:about="&smartsumo;partialOrderingOn"
	 rdfs:label="smartsumo:partialOrderingOn">
	<rdfs:comment>A BinaryRelation is a partial
ordering on a SetOrClass only if the relation is reflexiveOn the
SetOrClass, and it is both an AntisymmetricRelation, and a
TransitiveRelation.</rdfs:comment>
	<rdfs:domain rdf:resource="&smartsumo;BinaryRelation"/>
	<rdfs:range rdf:resource="&smartsumo;SetOrClass"/>
</rdf:Property>
<rdf:Property rdf:about="&smartsumo;partiallyFills"
