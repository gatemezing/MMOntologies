	<rdfs:domain rdf:resource="&smartsumo;weather-condition-pollen"/>
	<rdfs:range rdf:resource="&rdfs;Literal"/>
</rdf:Property>
<rdf:Property rdf:about="&smartsumo;polyline"
	 a:maxCardinality="1"
	 rdfs:label="smartsumo:polyline">
	<rdfs:comment>This slot originates in DTAG's RouteService.

The optional polyline of the route with coordinates. This can be use for calculating a station map with the map service.</rdfs:comment>
	<rdfs:domain rdf:resource="&smartsumo;Translocation"/>
	<rdfs:range rdf:resource="&rdfs;Literal"/>
</rdf:Property>
<rdf:Property rdf:about="&smartsumo;premiereDate"
	 rdfs:label="smartsumo:premiereDate">
	<rdfs:comment>This slot was introduce to capture the information of the DTAG's CinemaService</rdfs:comment>
	<rdfs:subPropertyOf rdf:resource="&smartdolce;PRESENT-AT"/>
