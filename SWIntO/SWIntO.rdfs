	<rdfs:domain rdf:resource="&smartsumo;weather-condition-pollen"/>
	<rdfs:range rdf:resource="&rdfs;Literal"/>
</rdf:Property>
<rdf:Property rdf:about="&smartsumo;pollen-name"
	 a:maxCardinality="1"
