	<rdfs:domain rdf:resource="&smartsumo;route-input-properties"/>
	<rdfs:range rdf:resource="&rdfs;Literal"/>
</rdf:Property>
<rdf:Property rdf:about="&smartsumo;overlapsPartially"
	 rdfs:label="smartsumo:overlapsPartially">
	<rdfs:comment>(overlapsPartially ?OBJ1 ?OBJ2) means
that ?OBJ1 and ?OBJ2 have part(s) in common, but neither ?OBJ1 nor ?OBJ2
is a part of the other.</rdfs:comment>
	<rdfs:subPropertyOf rdf:resource="&smartsumo;overlapsSpatially"/>
</rdf:Property>
<rdf:Property rdf:about="&smartsumo;overlapsSpatially"
	 rdfs:label="smartsumo:overlapsSpatially">
	<rdfs:comment>(overlapsSpatially ?OBJ1 ?OBJ2) means
that the Objects ?OBJ1 and ?OBJ2 have some parts in common.  This is a
reflexive and symmetric (but not transitive) relation.</rdfs:comment>
	<rdfs:subPropertyOf rdf:resource="&smartsumo;connected"/>
</rdf:Property>
<rdf:Property rdf:about="&smartsumo;parallel"
	 rdfs:label="smartsumo:parallel">
	<rdfs:comment>(parallel ?LINE1 ?LINE2) means that the
OneDimensionalFigures ?LINE1 and ?LINE2 are parallel to one another,
i.e. they are equidistant from one another at every point.</rdfs:comment>
	<rdfs:range rdf:resource="&smartsumo;OneDimensionalFigure"/>
	<rdfs:domain rdf:resource="&smartsumo;OneDimensionalFigure"/>
</rdf:Property>
<rdf:Property rdf:about="&smartsumo;parentalAdvisory"
